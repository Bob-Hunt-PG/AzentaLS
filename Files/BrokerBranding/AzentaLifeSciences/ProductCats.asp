	}

	function CurrencyFormatted(strCost)
	{
		var minus = '';
		var i = parseFloat(strCost);

		if(isNaN(i))i=0.00;
		if(i < 0)minus = '-';
		i = Math.abs(i);
		i = parseInt((i + .005) * 100);
		i = (i / 100);
		s = new String(i);
		if(s.indexOf('.') < 0)s += '.00';
		if(s.indexOf('.') == (s.length - 2))s += '0';
		s = minus + s;
		return s;
	}

