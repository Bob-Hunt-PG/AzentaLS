	function isWhitespace (strCurrent)
	{   
		var i;
		var whitespace = " \t\n\r";

		if (isEmpty(strCurrent)) return true;

		for (i = 0; i < strCurrent.length; i++)
		{   
			var c = strCurrent.charAt(i);

			if (whitespace.indexOf(c) == -1) return false;
		}

		return true;
	}

