				}
				else
				{
					aryCur2 = eval('aryLink' + aryCur[j]);
					for (var k=0; k<aryCur2.length; k++)
					{
						aryCur3 = aryCur2[k];
						blnAddIt=true;
						for (var l=0; l<aryCur3.length; l++)
						{
							blnAddIt = InArray(aryCur3[l], aryCurBoxValues);
							if (blnAddIt == false)break;
						}

