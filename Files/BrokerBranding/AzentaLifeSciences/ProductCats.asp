	function SetupBox(cboCur, aryOp, intIndex, intArrayIndex)
	{
		var aryCur;
		var aryCur2;
		var aryCur3;
		var aryCurBoxValues;
		var cboCur;
		var blnAddIt=true;

		aryCurBoxValues = eval('aryBoxValues' + intArrayIndex);

